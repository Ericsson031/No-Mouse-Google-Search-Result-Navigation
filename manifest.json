{
  "manifest_version": 2,
  "name": "No Mouse Google Search",
  "description": "Navigate google search results without mouse, only using keyboard hotkeys and shortcuts",
  "version": "1",

   "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["in.css"],
	  "js": ["parse.js"]
    }
  ],
  
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}