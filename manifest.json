{
  "manifest_version": 2,
  "name": "No Mouse Search Navigation",
  "description": "Navigate Google search results without mouse, using keyboard hotkeys and shortcuts",
  "version": "1",

   "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["core.css"],
	  "js": ["core.js", "lib/jquery-1.9.1.js"]
    }
  ],
  
  "options_page": "views/options.html",
  "browser_action": {
    "default_icon": "res/icon.png",
    "default_popup": "views/popup.html"
  }
}